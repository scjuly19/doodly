{
    "manifest_version":2,
    "name":"doodly",
    "version":"0.1",
    "content_scripts":[
        {
             "matches":["<all_urls>"],
             "js":["p5.js","p5.dom.min.js","content.js"]
        }
    ],
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"/popup/popup.html"
    },
    "background":{
        "scripts":["background.js"]
    },
    "permissions":["tabs","activeTab","storage"]
}